<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cst2335.groupproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".PkgHome.Home"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PkgActivity.ActivityInsert"
            android:windowSoftInputMode="stateVisible" />
        <activity android:name=".PkgActivity.ActivityUpdate" />
        <activity
            android:name=".PkgActivity.ActivityMenuHelp"
            android:label="@string/title_activity_menu_help"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>